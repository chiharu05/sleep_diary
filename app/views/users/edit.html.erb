<h1>登録情報の変更</h1>

<div class='account-page user-form'>
  <%= form_with model: current_user, local: true do |f|%>
    <div class='field'>
      <div class='field-label'>
        <%= f.label :email%>
      </div>
      <div class='field-input'>
        <%= f.text_field :email %>
      </div>
    </div>
    <div class='actions'>
      <%= f.submit "Update", class: 'btn'%>
    </div>
  <% end %>
</div>